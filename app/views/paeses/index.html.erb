<h1>Listing paeses</h1>

<table>
  <tr>
    <th>Descriz</th>
    <th>Tpeu</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @paeses.each do |paese| %>
  <tr>
    <td><%= paese.descriz %></td>
    <td><%= paese.tpeu %></td>
    <td><%= link_to 'Show', paese %></td>
    <td><%= link_to 'Edit', edit_paese_path(paese) %></td>
    <td><%= link_to 'Destroy', paese, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

