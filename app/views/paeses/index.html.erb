<%= render :partial => 'shared/page_title', :locals => {:record => nil} %>
<ul class="button-group">
  <li>
    <%= link_to 'NUOVO', new_paese_path, :class => "button radius small new foundicon-add-doc" %>
  </li>
</ul>
<% unless @paeses.nil?||@paeses.empty? %>
  <table>
    <thead>
      <tr>
        <th width="57%">Descrizione</th>
        <th width="10%">Tipo</th>
        <th width="8%">C.F.</th>
        <th width="10%">Pref.P.I.</th>
        <th width="15%"></th>
      </tr>
    </thead>
    <tbody>
        <%= render :partial => 'line', :collection => @paeses %>
    </tbody>
  </table>
  <%= will_paginate @paeses %>
<% end %>
