<%= render :partial => 'shared/page_title', :locals => {:record => nil} %>
<ul class="button-group">
  <li>
    <%= link_to 'AGGIUNGI DOC.', new_tesdoc_path(:causmag => @causmagfilter||"",
                                                 :conto   => @contofilter||""), :remote => true,
                                                 :class => "button radius small new foundicon-add-doc" %>
  </li>
<!--
  <li>
    <%= link_to 'IMPORTA DOCUMENTO', addtesrigdoc_fromxls_tesdocs_path, 
                :confirm => 'Confermi caricamento da Exel?', :class => "button radius small import foundicon-inbox" %>
  </li>
-->
</ul>
<%= render :partial => 'filter' %>
<% unless @tesdocs.nil?||@tesdocs.empty? %>
  <table>
    <thead>
      <tr>
        <th width="12%">Nr doc</th>
        <th width="13%">Del</th>
        <th width="30%">Causale</th>
        <th width="40%">ContoAnagrafica</th>
        <th width="5%"></th>
      </tr>
    </thead>
    <tbody>
        <%= render :partial => 'line', :collection => @tesdocs %>
    </tbody>
  </table>
  <%= will_paginate @tesdocs %>
<% end %>
