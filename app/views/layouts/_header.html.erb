<header>
  <% if signed_in? %>
    <div class="row">
      <ul class="nav-bar twelve columns">
        <li class="has-flyout">
          <%= link_to "DOCUMENTI", "#" %>
          <ul class="flyout">
            <% Tesdoc::TIPO_DOC.each do |k, v| %>
              <li> <%= link_to v.upcase, tesdocs_path(:tipo_doc => k) %> </li>
            <% end %>
          </ul>
        </li>
        <li class="has-flyout">
          <%= link_to "ANAGRAFICA", "#" %>
          <ul class="flyout">
            <li> <%= link_to "ARTICOLI", articles_path %> </li>
            <li> <%= link_to "CITTA'", localitas_path %> </li>
            <li> <%= link_to "SOGGETTI", anagens_path %> </li>
            <li> <%= link_to "IVA/ESENZIONI", ivas_path %> </li>
            <li> <%= link_to "STATI/NAZIONI", paeses_path %> </li>
          </ul>
        </li>
        <li class="has-flyout">
          <%= link_to "CAUSALI", "#" %>
          <ul class="flyout">
            <li> <%= link_to "CAUSALI CONTABILI", causales_path %> </li>
            <li> <%= link_to "CAUSALI MAGAZZINO", causmags_path %> </li>
          </ul>
        </li>
        <li class="has-flyout">
          <%= link_to "CONTI", "#" %>
          <ul class="flyout">
            <li> <%= link_to "CLIENTI/FORNITORI", contos_path %> </li>
          </ul>
        </li>
        <li class="active" style="float:right">
          <%= link_to "ESCI", signout_path, :method => :delete, :class=>"right" %>
        </li>
        <li class="active" style="float:right">
          <%= link_to "CAMBIA ANNO (#{current_annoese.to_s})", new_session_path(:login => current_user.login), :class=>"right" %>
        </li>
      </ul>
    </div>
  <% end %>
</header>
