desform = $("#anaind_desloc").val();
capform = $("#anaind_cap").val();
desdb = "<%=@descrizloc%>";
capdb = "<%=@caploc%>";
if (desform == "")
{ 
  $("#anaind_desloc").val(desdb);
  $("#anaind_cap").val(capdb);
}
else if (desform != desdb)
{
  if (confirm("vuoi sostituire " + desform + " con " + desdb + "?"))
  {
    $("#anaind_desloc").val(desdb);
    $("#anaind_cap").val(capdb);
  }
}
