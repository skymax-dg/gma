<div class="row">
  <nav class="top-bar">
    <ul>
      <!-- Title Area -->
      <li class="name">
      <h1>
        <%= link_to "G.M.A", root_path %>
      </h1>
      </li>
      <li class="toggle-topbar"><a href="#"></a></li>
    </ul>

    <section>
      <!-- Right Nav Section -->
      <ul class="left">
        <li class="divider"></li>
        <li class="has-dropdown">
          <a href="#" class="foundicon-add-doc">DOCUMENTI</a>
          <ul class="dropdown">
            <% Tesdoc::TIPO_DOC.each do |k, v| %>
              <li> <%= link_to v.upcase, tesdocs_path(:tipo_doc => k) %> </li>
            <% end %>
            <li> <%= link_to "SCADENZE", filter_scadenzas_path %> </li>
          </ul>
        </li>
        <li class="has-dropdown">
          <a href="#" class="foundicon-address-book">
            ANAGRAFICA
          </a>
          <ul class="dropdown">
            <li> <%= link_to "SOGGETTI", anagens_path %> </li>
            <li> <%= link_to "ARTICOLI", articles_path %> </li>
            <li> <%= link_to "CITTA'", localitas_path %> </li>
            <li> <%= link_to "STATI/NAZIONI", paeses_path %> </li>
            <li> <%= link_to "IVA/ESENZIONI", ivas_path %> </li>
            <li> <%= link_to "AGENTI", agentes_path %> </li>
          </ul>
        </li>
        <li class="has-dropdown">
          <a href="#" class="foundicon-settings">
