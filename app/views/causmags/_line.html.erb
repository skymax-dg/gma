<tr>
  <td><%= Causmag::TIPO_DOC[line.tipo_doc] %></td>
  <td><%= line.descriz %></td>
  <td><%= Causmag::TIPO[line.tipo] %></td>
  <td><%= Causmag::MOVIMPMAG[line.movimpmag] %></td>
  <td><%= Causmag::CONTABILE[line.contabile] %></td>
  <td><%= line.causale.descriz unless line.causale.nil? %></td>
  <td><%= link_to 'Visualizza', line %></td>
  <td><%= link_to 'Modifica', edit_causmag_path(line) %></td>
  <td><%= link_to 'Elimina', line, :confirm => 'Confermi?', :method => :delete %></td>
</tr>
